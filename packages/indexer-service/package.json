{
  "name": "@graphprotocol/indexer-service",
  "version": "0.1.0",
  "description": "Indexer service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/graphprotocol/indexer",
  "author": "Graph Protocol",
  "private": false,
  "scripts": {
    "format": "prettier --write src/*.ts src/**/*.ts src/**/**/*.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "yarn format && yarn lint && tsc",
    "start": "yarn prepare && node ./dist/index.js start",
    "test": "jest --passWithNoTests --detectOpenHandles --verbose",
    "test:watch": "jest --watch --passWithNoTests --detectOpenHandles --verbose"
  },
  "bin": {
    "graph-indexer-service": "bin/graph-indexer-service"
  },
  "resolutions": {
    "scrypt": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz"
  },
  "dependencies": {
    "@statechannels/server-wallet": "^0.3.2",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bs58": "^4.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.4",
    "ethers": "5.0.7",
    "evt": "^1.7.13",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^5.0.0",
    "morgan": "^1.9.1",
    "p-queue": "^6.4.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@statechannels/devtools": "^0.3.0",
    "@types/body-parser": "^1.19.0",
    "@types/bs58": "^4.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/supertest": "^2.0.10",
    "nock": "^13.0.3",
    "supertest": "^4.0.2"
  }
}
