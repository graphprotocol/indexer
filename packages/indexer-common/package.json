{
  "name": "@graphprotocol/indexer-common",
  "version": "0.21.0",
  "description": "Common library for Graph Protocol indexer components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": "https://github.com/graphprotocol/indexer",
  "author": "The Graph Foundation",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "compile": "tsc",
    "prepare": "yarn format && yarn lint && yarn compile",
    "test": "POSTGRES_TEST_HOST=\"localhost\" POSTGRES_TEST_PORT=\"5432\" POSTGRES_TEST_USERNAME=\"root\" POSTGRES_TEST_PASSWORD=\"root\" POSTGRES_TEST_DATABASE=\"blobs\" LOG_LEVEL=info jest --colors --verbose --forceExit --runInBand",
    "test:ci": "LOG_LEVEL=error jest --verbose --runInBand --forceExit --ci",
    "test:debug": "LOG_LEVEL=debug jest --runInBand --detectOpenHandles --verbose --forceExit",
    "test:watch": "jest --runInBand --detectOpenHandles --watch --passWithNoTests --verbose",
    "clean": "rm -rf ./node_modules ./dist ./tsconfig.tsbuildinfo"
  },
