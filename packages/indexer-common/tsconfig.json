{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "es2020",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "composite": true,
    "lib": [
      "esnext.asynciterable",
      "dom",
      "es2020.promise",
      "es2015.iterable",
      "es2015.promise"
    ],
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [],
  "references": []
}
